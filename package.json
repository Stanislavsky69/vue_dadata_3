{
  "name": "vue-dadata-3",
  "version": "2.1.0",
  "private": false,
  "description": "Компонент для работы с dadata для vue 3",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./index.css": "./dist/index.css",
    "./styles.scss": "./dist/styles.scss"
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "vue3",
    "vuejs",
    "dadata",
    "composition_api",
    "vue",
    "vuex",
    "vite",
    "vue-plugin"
  ],
  "author": {
    "email": "plugin69@bk.ru",
    "name": "Stanislav Odnoletko"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Stanislavsky69/vue_dadata_3.git"
  },
  "bugs": {
    "url": "https://github.com/Stanislavsky69/vue_dadata_3/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "build": "vue-tsc --noEmit && vite build && node ./scripts/replaceScssFiles.js",
    "test:run": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@babel/types": "^7.18.2",
    "@types/debounce": "^1.2.1",
    "@types/eslint": "^8.4.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-dom": "^3.2.36",
    "@vue/eslint-config-standard": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "axios-mock-adapter": "^1.21.1",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jsdom": "^19.0.0",
    "sass": "^1.52.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vite-plugin-dts": "^1.2.0",
    "vite-plugin-eslint": "^1.6.1",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.6"
  }
